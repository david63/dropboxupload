{
	"name": "david63/dropboxupload",
	"type": "phpbb-extension",
	"description": "Extends autobackup to allow the copying of the backup file to your Dropbox account.",
	"version": "2.1.0-RC5",
	"time": "2021-01-14",
	"keywords": [
		"phpbb",
		"extension",
		"autobackup",
		"Dropbox"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		},
        {
            "name": "Kunal Varma",
            "homepage": "https://github.com/kunalvarma05/dropbox-php-sdk",
            "role": "Creator - Dropbox stk"
        }
    ],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0",
		"kunalvarma05/dropbox-php-sdk": "^0.2.1"
    },
	"extra": {
		"display-name": "[d63] Dropbox upload for autobackup",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "dropbox_upload_version_file.json",
			"ssl": false
    	}
	}
}
